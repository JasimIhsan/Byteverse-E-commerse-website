                                                    <div class="filter-item">
                                                        <div class="custom-control custom-checkbox">
                                                            <input type="checkbox" class="custom-control-input" id="brand-14" value="Xiaomi" />
                                                            <label class="custom-control-label" for="brand-14">Xiaomi</label>
                                                        </div>
                                                        <!-- End .custom-checkbox -->
                                                    </div>
                                                    <!-- End .filter-item -->

                                                    <div class="filter-item">
                                                        <div class="custom-control custom-checkbox">
                                                            <input type="checkbox" class="custom-control-input" id="brand-15" value="Alienware" />
                                                            <label class="custom-control-label" for="brand-15">Alienware</label>
