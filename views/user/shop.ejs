                                                        <div class="custom-control custom-checkbox">
                                                            <input type="checkbox" class="custom-control-input" id="brand-4" value="Lenovo" />
                                                            <label class="custom-control-label" for="brand-4">Lenovo</label>
                                                        </div>
                                                        <!-- End .custom-checkbox -->
                                                    </div>
                                                    <!-- End .filter-item -->

                                                    <div class="filter-item">
                                                        <div class="custom-control custom-checkbox">
                                                            <input type="checkbox" class="custom-control-input" id="brand-5" value="Asus" />
                                                            <label class="custom-control-label" for="brand-5">Asus</label>
                                                        </div>
                                                        <!-- End .custom-checkbox -->
                                                    </div>
                                                    <!-- End .filter-item -->

                                                    <div class="filter-item">
                                                        <div class="custom-control custom-checkbox">
                                                            <input type="checkbox" class="custom-control-input" id="brand-6" value="Acer" />
                                                            <label class="custom-control-label" for="brand-6">Acer</label>
                                                        </div>
                                                        <!-- End .custom-checkbox -->
