                                                    <!-- End .filter-item -->

                                                    <div class="filter-item">
                                                        <div class="custom-control custom-checkbox">
                                                            <input type="checkbox" class="custom-control-input" id="brand-12" value="Sony" />
                                                            <label class="custom-control-label" for="brand-12">Sony</label>
                                                        </div>
                                                        <!-- End .custom-checkbox -->
