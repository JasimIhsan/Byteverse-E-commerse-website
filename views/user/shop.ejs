                                                    </div>
                                                    <!-- End .filter-item -->

                                                    <div class="filter-item">
                                                        <div class="custom-control custom-checkbox">
                                                            <input type="checkbox" class="custom-control-input" id="brand-3" value="HP" />
                                                            <label class="custom-control-label" for="brand-3">HP</label>
                                                        </div>
                                                        <!-- End .custom-checkbox -->
                                                    </div>
                                                    <!-- End .filter-item -->

