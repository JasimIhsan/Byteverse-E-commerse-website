                                                    <!-- End .filter-item -->

                                                    <div class="filter-item">
                                                        <div class="custom-control custom-checkbox">
                                                            <input type="checkbox" class="custom-control-input" id="brand-13" value="Huawei" />
                                                            <label class="custom-control-label" for="brand-13">Huawei</label>
