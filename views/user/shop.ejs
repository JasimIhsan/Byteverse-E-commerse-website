                                                            <input type="checkbox" class="custom-control-input" id="brand-10" value="Samsung" />
                                                            <label class="custom-control-label" for="brand-10">Samsung</label>
                                                        </div>
                                                        <!-- End .custom-checkbox -->
                                                    </div>
                                                    <!-- End .filter-item -->

                                                    <div class="filter-item">
                                                        <div class="custom-control custom-checkbox">
                                                            <input type="checkbox" class="custom-control-input" id="brand-11" value="Toshiba" />
